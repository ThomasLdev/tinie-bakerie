{
  "name": "tinie-bakerie",
  "version": "1.0.0",
  "description": "E2E tests for Tinie Bakerie",
  "scripts": {
    "test:e2e": "playwright test",
    "test:e2e:debug": "PWDEBUG=1 playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report --host 0.0.0.0 --port 9323",
    "lint": "eslint assets/controllers/**/*.ts",
    "lint:fix": "eslint assets/controllers/**/*.ts --fix",
    "format": "prettier --write \"assets/controllers/**/*.ts\"",
    "format:check": "prettier --check \"assets/controllers/**/*.ts\"",
    "type-check": "tsc --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@hotwired/stimulus": "^3.2.2",
    "@lhci/cli": "^0.15.0",
    "@playwright/test": "^1.48.0",
    "@types/node": "^22.9.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.3.3",
    "swiper": "^11.1.15",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.15.0"
  }
}
